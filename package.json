{
	"name": "@rx1310/kalium19",
	"version": "0.0.5",
	"description": "Набор утилит, миксинов, расширений на языке препроцессора @sass для помощи в ускорении и упрощении написания CSS-стилей.",
	"main": "_kalium19.scss",
	"var": "alpha",
	"publishConfig": {
    "access": "public"
  },
	"scripts": {
		"sass:compile": "sass . --style compressed",
		"sass:watch": "sass --watch .",
		"sass:watch-compressed": "sass --watch . --style compressed",
		"editorconfig": "editorconfig-checker"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/91muilak/kalium19.git"
	},
	"funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/rx1310"
  },
	"keywords": [
		"kalium19",
		"91muilak",
		"k19",
		"CSS",
		"SASS",
		"SCSS"
	],
	"author": {
    "name": "Haba Kudzaev",
    "email": "rx1310@inbox.ru",
    "url": "https://rx1310.github.io"
  },
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/91muilak/kalium19/issues"
	},
	"homepage": "https://github.com/91muilak/kalium19#readme",
	"devDependencies": {
		"editorconfig-checker": "^4.0.2",
		"npm-run-all": "^4.1.5",
		"sass": "^1.44.0"
	}
}