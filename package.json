{
	"name"       : "@rx1310/kalium19",
	"version"    : "0.0.8",
	"description": "Набор утилит, миксинов, расширений на языке препроцессора @sass для помощи в ускорении, упрощении и улучщения процесса написания CSS-стилей.",
	"main"       : "_k19.scss",
	"var"        : "alpha",
	"publishConfig": {
    "access": "public"
  },
	"scripts": {
		"sass:compile"         : "sass --no-source-map . --style compressed",
		"sass:watch"           : "sass --no-source-map --watch .",
		"sass:watch-compressed": "sass --watch . --style compressed"
	},
	"repository": {
		"type": "git",
		"url" : "git+https://github.com/91muilak/kalium19.git"
	},
	"funding": {
    "type": "opencollective",
    "url" : "https://opencollective.com/rx1310"
  },
	"keywords": [
		"kalium19",
		"91muilak",
		"k19",
		"CSS",
		"SASS",
		"SCSS"
	],
	"author": {
    "name" : "Haba Kudzaev",
    "email": "rx1310@inbox.ru",
    "url"  : "https://rx1310.github.io"
  },
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/91muilak/kalium19/issues"
	},
	"homepage": "https://github.com/91muilak/kalium19#readme",
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"sass": "1.49.0"
	}
}